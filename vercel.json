{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node@2.5.10",
      "config": {
        "installCommand": "npm install -g yarn@1.22.19 && yarn install",
        "buildCommand": "yarn build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "engines": {
    "node": "18.x",
    "yarn": "1.22.x"
  }
}
